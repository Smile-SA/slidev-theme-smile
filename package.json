{
  "name": "slidev-theme-smile",
  "version": "0.0.0",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/client": "^0.28.1",
    "@slidev/types": "^0.28.1",
    "codemirror-theme-vars": "^0.1.1",
    "prism-theme-vars": "^0.2.2"
  },
  "devDependencies": {
    "@iconify-json/mdi": "^1.0.12",
    "@iconify-json/uim": "^1.0.2",
    "@slidev/cli": "^0.28.1"
  },
  "engines": {
    "node": ">=14.0.0",
    "slidev": ">=0.19.3"
  },
  "slidev": {
    "colorSchema": "both",
    "highlighter": "shiki",
    "lineNumbers": true,
    "defaults": {
      "fonts": {
        "sans": "Roboto",
        "mono": "Roboto Mono",
        "weights": "200,400,600,900",
        "fallback": false
      }
    }
  }
}
